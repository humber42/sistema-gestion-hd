                     <accelerator>
                        <KeyCodeCombination alt="DOWN" code="R" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                        </MenuItem>
                  <MenuItem mnemonicParsing="false" onAction="#bajaPase" text="Dar baja pase" />

