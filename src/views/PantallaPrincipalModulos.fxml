                                    <image>
                                        <Image url="@../icons/icons8_Shutdown_50px.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </JFXButton>
                        <Label layoutX="357.0" layoutY="9.0" text="Sistema Informativo ETECSA" textFill="WHITE">
