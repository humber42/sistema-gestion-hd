                                <Font size="14.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="44.0" fitWidth="61.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/icons8_POS_Terminal_50px_1.png"/>
