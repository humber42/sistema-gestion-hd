                            <graphic>
                                <ImageView fitHeight="51.0" fitWidth="68.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/icons8_Agent_Smith_50px.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </JFXButton>
                        <ImageView fitHeight="86.0" fitWidth="105.0" layoutX="48.0" layoutY="14.0" pickOnBounds="true"
