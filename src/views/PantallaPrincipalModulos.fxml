                            <image>
                                <Image url="@../icons/etecsa_icon.jpg"/>
                            </image>
                        </ImageView>
                    </children>
                </Pane>
            </left>
            <top>
                <Pane prefHeight="42.0" prefWidth="934.0" styleClass="barra-superior"
