<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="154.0" prefWidth="100.0"
           xmlns="http://javafx.com/javafx/9.0.1" fx:controller="views.dialogs.DialogLoadingController2">
    <children>
        <AnchorPane prefHeight="149.0" prefWidth="100.0">
            <children>
                <Label fx:id="loading" alignment="CENTER" layoutX="2.0" layoutY="78.0" text="Cargando..."
                       textFill="#000000" AnchorPane.leftAnchor="2.0" AnchorPane.rightAnchor="-2.0">
                    <font>
                        <Font size="18.0"/>

                    </font>
                </Label>
                <ProgressIndicator layoutY="9.0" prefHeight="66.0" prefWidth="100.0" style="-fx-accent: #000000;"
                                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"/>
                <JFXButton buttonType="RAISED" layoutX="19.0" layoutY="114.0" onAction="#cancelProcess"
                           style="-fx-background-color: red;" text="Cancelar" textFill="WHITE">
                    <font>
                        <Font size="13.0"/>
                    </font>
                </JFXButton>
            </children>
        </AnchorPane>
    </children>
</StackPane>
