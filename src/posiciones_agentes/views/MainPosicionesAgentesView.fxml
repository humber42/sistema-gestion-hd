<BorderPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="posiciones_agentes.views.MainPosicionesAgentesController">
    <center>
        <AnchorPane minHeight="583.0" prefWidth="934.0">
